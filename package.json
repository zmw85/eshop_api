{
  "name": "eshop_api",
  "version": "1.0.0",
  "description": "eshop api",
  "config": {
    "s3BucketName": "aws-serverless-lambda",
    "region": "ap-southeast-2",
    "cloudFormationStackName": "AwsServerlessExpressStack",
    "lambdaFunctionName": "eshop_express_lambda"
  },
  "scripts": {
    "test": "mocha ./test/auth/token",
    "start": "node server/server.js",
    "create-bucket": "aws s3 mb s3://$npm_package_config_s3BucketName --region $npm_package_config_region",
    "delete-bucket": "aws s3 rb s3://$npm_package_config_s3BucketName --region $npm_package_config_region",
    "deploy-lambda": "sh lambda-deploy.sh",
    "scale-lambda": "aws lambda update-function-configuration --function-name $npm_package_config_lambdaFunctionName --memory-size $1"
  },
  "author": "Zhengmin Wang",
  "license": "ISC",
  "dependencies": {
    "aws-serverless-express": "^3.0.2",
    "bcrypt": "^1.0.2",
    "body-parser": "^1.17.1",
    "express": "^4.15.2",
    "express-jwt": "^5.1.0",
    "express-validator": "^3.2.0",
    "jsonwebtoken": "^7.3.0",
    "moment": "^2.18.1",
    "morgan": "^1.8.2",
    "mysql": "^2.13.0",
    "rxjs": "^5.3.0",
    "sequelize": "^3.30.4",
    "uuid": "^3.0.1"
  },
  "devDependencies": {
    "chai": "^4.0.2",
    "chai-http": "^3.0.0",
    "gulp": "^3.9.1",
    "gulp-eslint": "^3.0.1",
    "gulp-nodemon": "^2.2.1",
    "mocha": "^3.4.2",
    "nodemon": "^1.11.0"
  }
}
